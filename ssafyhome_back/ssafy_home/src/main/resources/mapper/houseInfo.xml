<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.home.house.model.mapper.HouseInfoMapper">

    <!-- 전체 데이터 조회 -->
    <select id="getAllHouses" resultType="com.ssafy.home.house.model.HouseInfoDto">
        SELECT
            apt_seq AS aptSeq,
            sgg_cd AS sggCd,
            umd_cd AS umdCd,
            umd_nm AS umdNm,
            jibun,
            road_nm AS roadNm,
            apt_nm AS aptNm,
            build_year AS buildYear,
            latitude,
            longitude
        FROM
            houseinfos
    </select>

</mapper>
