<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.home.article.model.mapper.GuideMapper">

    <!-- 가이드 카드 전체 조회 -->
    <select id="selectAllGuides" resultType="com.ssafy.home.article.model.GuideDto">
        SELECT id, category, content, url
        FROM guide
        ORDER BY id ASC
    </select>

</mapper>